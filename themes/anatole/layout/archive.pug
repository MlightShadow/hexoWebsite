extends partial/nlayout

block site_title
    !=  __('Archive') + " · " + config.title

block description
    - var desc = __('Archive') + '页面 - ' + config.title;
    meta(name="description", content=desc)

block content
    .autopagerize_page_element: .content
      .archive.animated.fadeInDown
        ul.list-with-title
          include mixins
          +postList()
          +make_pager(__('prev'), __('next'))
